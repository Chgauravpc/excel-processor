{
    "version": 2,
    "builds": [
        {
            "src": "public/**",
            "use": "@vercel/static"
        },
        {
            "src": "api/process_excel.py",
            "use": "@vercel/python",
            "config": { "runtime": "python3.9" }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/process_excel.py"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}